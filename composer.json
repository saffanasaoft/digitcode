{
    "name": "digitcode/digitcodeflazz",
    "description": "Digitcode Client Bwallet",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.23"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Digitcode\\Digitcodeflazz\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitcode\\Digitcodeflazz\\DigitcodeServiceProvider"
            ],
            "aliases": {
                "Digitcode": "Digitcode\\Digitcodeflazz\\Digitcode"
            }
        }
    }
}
